mv -f /data/media/0/.Kernel/4.14.15-PhoenixOS-x86_64 /system/lib/modules/
find /system/lib -type d -exec chmod 755 {} \;
find /system/lib -type f -exec chmod 644 {} \;
y=$(find /mnt/runtime/default -maxdepth 4 -type f -name ramdisk.img)
z=${y%/*}
cd /data/media/0/Animus
chmod 777 kernel
cd /data/media/0/Animus
rsync -aq kernel $z
find /data/media/0/Animus -type d -exec chmod 777 {} \;
find /data/media/0/Animus/modules -type f -exec chmod 777 {} \;
cd /data/media/0/Animus
X=/data/media/0/Animus/firmware
if [ -d "$X" ]; then
find /data/media/0/Animus/firmware -type f -exec chmod 777 {} \;
fi
cd /storage/emulated/0/Animus
rm -f *.zip
cd /storage/emulated/0/Animus
rm -rf modules firmware kernel
cd /storage/emulated/0
rmdir .Kernel
sleep 3
echo -e "\e[92;1m[ IMAGINATION IS YOUR ONLY LIMIT. ]\e[0m"
echo -e " "
echo -e "\e[98;1m[ REBOOTING ]\e[0m"
sleep 3
