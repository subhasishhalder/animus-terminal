do
clear
echo -e "\e[91;1m* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * \e[0m"
echo -e " "
echo -e "\e[94;1m"
cat<< "EOF"
                                _____ __________  _________________________________________  ________ ________    ________    _________
                               /  _  \\______   \/   _____/\__    ___/\_   _____/\______   \/  _____/ \_____  \   \_____  \  /   _____/
                              /  /_\  \|    |  _/\_____  \   |    |    |    __)_  |       _/   \  ___  /   |   \   /   |   \ \_____  \ 
                             /    |    \    |   \/        \  |    |    |        \ |    |   \    \_\  \/    |    \ /    |    \/        \
                             \____|__  /______  /_______  /  |____|   /_______  / |____|_  /\______  /\_______  / \_______  /_______  /
                                     \/       \/        \/                    \/         \/        \/         \/          \/        \/ 
EOF
echo -e "\e[0m"
echo -e "\e[91;1m* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * \e[0m"
sleep 0.1
echo -e " "
s1=$(df /data/ | awk '/^\/dev/ {print $1}')
s2=$(date)
m1=$(cat /system/usr/share/alsa/pcm/aos.txt)
st=$(cat /system/usr/share/alsa/pcm/animus.txt)
echo -e "\e[0;1m \e[96;1m[ Operating System :-  "$m1"  ]\e[0m"
sleep 0.1
echo -e "\e[96;1m [ Animus Version:- $st ]\e[0m"
sleep 0.1
echo -e "\e[0;1m \e[96;1m[ Current Date & time :- [$s2] ]\e[0m"
sleep 0.1
echo -e " "
echo -e " "
echo -e " "
echo -e "                                                             \e[94;1m* * * * * * * * * * * * * * * * * * * * * * * * * *\e[0m"
echo -e "                                                             \e[91;1m*                WELCOME TO ANIMUS                *\e[0m"
echo -e "                                                             \e[94;1m* * * * * * * * * * * * * * * * * * * * * * * * * *\e[0m"
echo -e " "
echo -e " "
echo -e " "
sleep 0.1
echo -e "                                                              \e[91;1m [1] INSTALLER.\e[0m"
sleep 0.1
echo -e " "
echo -e "                                                              \e[94;1m [2] BEAST MODE.\e[0m"
sleep 0.1
echo -e " "
echo -e "                                                              \e[91;1m [3] GL TOOL MODE.\e[0m"
sleep 0.1
echo -e " "
echo -e "                                                              \e[94;1m [4] GAME-ZONE.\e[0m"
sleep 0.1
echo -e " "
echo -e "                                                              \e[91;1m [5] SYSTEM TWEAKS.\e[0m"
sleep 0.1
echo -e " "
echo -e "                                                              \e[94;1m [6] ANIMUS SPECIAL.\e[0m"
sleep 0.1
echo -e " "
echo -e "                                                              \e[91;1m [e] EXIT ANIMUS.\e[0m"
sleep 0.1
echo -e " "
echo -e " "
echo -e " "
  echo -e "\e[91;1m* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * \e[0m"
  echo -e "\e[94;1m [a] About.                  [r] Reboot.                 [s] System Specs. Beta.                 [h] Power-Off.                  [c] Credits.                    [u] Contact Us.\e[0m"
  echo -e "\e[91;1m* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * \e[0m"
  echo -n "\e[98;1mEnter Your Menu Choice [1-6]:"
echo -e " "
read c
clear
case $c in
1)
echo -e "\e[91;1m* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *\e[0m"
echo -e "\e[94;1m*                                                                 *"
cat <<"EOF"
*  _____ _   _  _____ _______       _      _      ______ _____    *
* |_   _| \ | |/ ____|__   __|/\   | |    | |    |  ____|  __ \   *
*   | | |  \| | (___    | |  /  \  | |    | |    | |__  | |__) |  *
*   | | | . ` |\___ \   | | / /\ \ | |    | |    |  __| |  _  /   *
*  _| |_| |\  |____) |  | |/ ____ \| |____| |____| |____| | \ \   *
* |_____|_| \_|_____/   |_/_/    \_\______|______|______|_|  \_\  *
*                                                                 *
EOF
echo -e "*                                                                 *\e[0m"
echo -e "\e[91;1m* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *\e[0m"
echo -e "\e[94;1m[ Choose Your Options Here ]\e[0m"
echo -e " "
echo -e "\e[91;1m [1] KERNEL.\e[0m"
echo -e " "
echo -e "\e[94;1m [2] MESA.\e[0m"
echo -e " "
echo -e "\e[91;1m [3] ARM.\e[0m"
echo -e " "
echo -e "\e[94;1m [4] FIRMWARE. \e[0m"
echo -e " "
echo -e "\e[91;1m [5] INIT. \e[0m"
echo -e " "
echo -e "\e[94;1m [6] BUILD.PROP. \e[0m"
echo -e " "
echo -e "\e[91;1m [7] Return To Main Menu.\e[0m"
echo -e " "
echo -e "\e[95;1m[ Enter Your Option Here :- ]\e[0m"
read c
clear
case $c in
1)
echo -e "\e[91;1m* * * * * * * * * * * * * * * * * * * * * * *\e[0m"
echo -e "\e[94;1m*                                           *"
cat<< "EOF"
*  _  ________ _____  _   _ ______ _        *
* | |/ /  ____|  __ \| \ | |  ____| |       *
* | ' /| |__  | |__) |  \| | |__  | |       *
* |  < |  __| |  _  /| . ` |  __| | |       *
* | . \| |____| | \ \| |\  | |____| |____   *
* |_|\_\______|_|  \_\_| \_|______|______|  *                                        
*                                           *
EOF
echo -e "*                                           *\e[0m"
echo -e "\e[91;1m* * * * * * * * * * * * * * * * * * * * * * *\e[0m"
echo -e " "
echo -e "\e[94;1m PRESS ENTER TO CONTINUE ]\e[0m"
echo -e " "
read c
if [ -e /data/media/0/Animus/*.zip ]; then
echo -e  "\e[91;1m UPDATING KERNEL \e[0m"
echo -e  " "
cd /data/media/0/Animus/
mv *.zip Kernel.zip
unzip -oq Kernel
else
echo -e  "\e[91;1m[ KERNEL NOT FOUND, PUT KERNEL<versioncode>.zip INSIDE Animus ; Error 69 ]\e[0m"
echo -e  " "
fi
echo -e "\e[95;1m[ CHOOSE YOUR PARTITION STYLE ]\e[0m"
echo -e " "
echo -e "\e[91;1m [1] EXT4  \e[0m"
echo -e " "
echo -e "\e[94;1m [2] NTFS \e[0m"
echo -e " "
read c
clear
case $c in
1)
cd /data/media/0/Animus
A=/data/media/0/Animus/firmware
if [ -d "$A" ]; then
cd /system/lib
rm -rf firmware
find /data/media/0/Animus -type d -exec chmod 755 {} \;
find /data/media/0/Animus/firmware -type f -exec chmod 644 {} \;
rsync -aq /data/media/0/Animus/firmware /system/lib/
else
echo ""
fi
cd /data/media/0/Animus
B=/data/media/0/Animus/modules
if [ -d "$B" ]; then
echo -e  "\e[91;1m[ BACKING UP STOCK KERNEL ] \e[0m"
echo -e  " "
mkdir -m 755 /data/media/0/.Kernel
cd /system/lib/modules
rsync -aq 4.14.15-PhoenixOS-x86_64 /data/media/0/.Kernel
cd /system/lib
rm -rf modules
find /data/media/0/Animus -type d -exec chmod 755 {} \;
find /data/media/0/Animus/modules -type f -exec chmod 644 {} \;
rsync -aq /data/media/0/Animus/modules /system/lib/
mv -f /data/media/0/.Kernel/4.14.15-PhoenixOS-x86_64 /system/lib/modules/
find /system/lib -type d -exec chmod 755 {} \;
find /system/lib -type f -exec chmod 644 {} \;
x=$(df /data/ | awk '/^\/dev/ {print $1}')
mkdir -m 755 /data/media/0/ext4
mount -t ext4 -rw $x /data/media/0/ext4
y=$(find /data/media/0/ext4 -maxdepth 4 -type f -name ramdisk.img)
z=${y%/*}
cd /data/media/0/Animus
chmod 777 kernel
cd /data/media/0/Animus
rsync -aq kernel $z
find /data/media/0/Animus -type d -exec chmod 777 {} \;
find /data/media/0/Animus/modules -type f -exec chmod 777 {} \;
cd /data/media/0/Animus
X=/data/media/0/Animus/firmware
if [ -d "$X" ]; then
find /data/media/0/Animus/firmware -type f -exec chmod 777 {} \;
fi
cd /storage/emulated/0/Animus
rm -f *.zip
cd /storage/emulated/0/Animus
rm -rf modules firmware kernel
cd /storage/emulated/0
rmdir .Kernel
umount -t ext4 $x
cd /data/media/0
rmdir ext4
echo -e "\e[91;1m[ IMAGINATION IS YOUR ONLY LIMIT ]\e[0m"
echo -e " "
echo -e "\e[94;1m[ REBOOTING ]\e[0m"
sleep 5
reboot
else
echo -e  "\e[91;1m[ KERNEL SOURCE IS DIFFERENT, CAN'T UPDATE, PLEASE DOWNLOAD FROM LINKS PROVIDED IN ABSTERGO UNIVERSE DISCORD SERVER ONLY ; Error 69 ]\e[0m"
cd /storage/emulated/0/Animus
rm -f *.zip
cd /storage/emulated/0/Animus
rm -rf modules firmware kernel
fi
;;
2)
cd /data/media/0/Animus
A=/data/media/0/Animus/firmware
if [ -d "$A" ]; then
cd /system/lib
rm -rf firmware
